    <connection>sls_raw</connection>
    <sql>SELECT 
	s.sale_id
	, s.date AS sale_date
	, s.customer_id
	, s.lens_id
	, s.amount AS sales
	, sc.sales_channel
	FROM 
	sls_raw.sale s
	LEFT JOIN
	sls_raw.sales_channel sc
	ON
	sc.sales_channel_id = s.sales_channel_id
	WHERE
	s.sale_id &gt; ${MAX_FACT_SALE_ID}
	ORDER BY sale_date ASC
	;
</sql>
